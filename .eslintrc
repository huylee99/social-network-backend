{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "prettier/prettier": [
            "warn",
            {
                "arrowParens": "always",
                "semi": true,
                "trailingComma": "all",
                "singleQuote": false,
                "printWidth": 120,
                "tabWidth": 4,
                "useTabs": true,
                "jsxSingleQuote": false
            }
        ]
    }
}
